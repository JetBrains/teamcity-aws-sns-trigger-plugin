<?xml version="1.0" encoding="UTF-8"?>


<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
    <info>
        <name>aws-sns-trigger</name>
        <display-name>Amazon SNS Trigger</display-name>
        <version>@Plugin_Version@</version>
        <description>Enables build triggering via SNS notification</description>
        <download-url>https://github.com/JetBrains/teamcity-aws-sns-trigger-plugin</download-url>
        <vendor>
            <name>JetBrains</name>
            <url>https://www.jetbrains.com/</url>
        </vendor>
    </info>
    <deployment use-separate-classloader="true" node-responsibilities-aware="true"/>
</teamcity-plugin>